<h1>All Users</h1>
<a asp-action="Create">Create User</a>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (MamaFood.Areas.Identity.Data.MamaFoodUser user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>

</table>
